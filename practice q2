#include <stdio.h>
int main()
{
   int i,j,n;
    printf("enter size of array");
    scanf("%d",&n);
    int a[n];
    printf("enter elements ");
    for(i=0;i<n;i++)
    scanf("%d",&a[i]);
    
      if (n == 1) 
        return; 
   for (int i = 0; i < n - 1; i++) { 
  if ((a[i] != 0) && (a[i] == a[i + 1])) { 
  a[i] = 2 * a[i];
 a[i + 1] = 0;
     i++; 
        }  } 
  int count = 0; 
   for (int i = 0; i < n; i++) 
        if (a[i] != 0) 
   a[count++] = a[i];
    
    while (count < n) 
        a[count++] = 0; 
     
    printf("the array is");
for(i=0;i<n;i++)
printf("%d ", a[i] );
   return 0;}

